FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      surfaceFeaturesDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// DONE: Geben Sie hier den Namen der Geometrie-Datei aus constant/triSurfae ein.
surfaces
(
    "domain_a3_b1.stl"
);

includedAngle   150;

subsetFeatures
{
    nonManifoldEdges yes;
    openEdges        yes;
}

trimFeatures
{
    minElem         0;
    minLen          0;
}

writeObj        yes;

